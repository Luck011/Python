sal = int(input("Qual sal치rio?\n"))
reajuste = int(input("reajuste?\n"))

res = sal * reajuste / 100
resFinal = res + sal

print("Sal치ri final\n" + str(resFinal))

------------------------------------
# Resultado final inteiro


sal = int(input("Qual sal치rio?\n"))
reajuste = int(input("reajuste?\n"))

res = sal * reajuste / 100
resFinal = res + sal

print("Sal치ri final\n" + str(int(resFinal)))

